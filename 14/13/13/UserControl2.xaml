<UserControl x:Class="WpfApp2.UserControl2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfApp2"
             mc:Ignorable="d" 
             d:DesignHeight="288" d:DesignWidth="148.8">
    <Grid>
        <Rectangle HorizontalAlignment="Left" Height="225.6" Margin="64.8,62.4,-82.8,-288" VerticalAlignment="Top" Width="18">
            <Rectangle.Fill>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FF690A0A" Offset="1"/>
                    <GradientStop Color="#FFCB1E1E"/>
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
        <Ellipse x:Name="EL"  HorizontalAlignment="Left" Height="166.8" Margin="0,0,-148.8,-166.8" VerticalAlignment="Top" Width="148.8">
            <Ellipse.Fill>
                <RadialGradientBrush>
					<GradientStop x:Name="Dark" Color="#FF2E5C0C" Offset="0.987"/>
					<GradientStop x:Name="Light" Color="#FF57F90E" Offset="0.344"/>
                </RadialGradientBrush>
            </Ellipse.Fill>
        </Ellipse>

    </Grid>
</UserControl>
