<Window x:Class="_13.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_13"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<Grid>
		<DockPanel>
			<StackPanel Width="120" Margin="5" HorizontalAlignment="Left">
				<Button Click="create_click" Content="Создать" Background="Red" Foreground="White" FontSize="16" FontFamily="Segoe Print Bold" Margin="5"/>
				<Button Click="createComplex_click" Content="Комплекс" Background="Red" Foreground="White" FontSize="16" FontFamily="Segoe Print Bold" Margin="5"/>
			</StackPanel>
			<Grid x:Name="grid">

			</Grid>
		</DockPanel>
	</Grid>
</Window>
